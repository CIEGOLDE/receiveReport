<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog id="SupplierDialog" noDataText="No Value Found " title="{i18n>supplierTitle}" multiSelect="true" confirm="onHandleClose" liveChange="onliveChang" cancel="onhandleClose2"
 		items="{ path : '/supplierSet', sorter : { path : 'Supplier', descending : false } }"  
 		search="handleSearch">
			<ColumnListItem>
			<cells>		
				<Text text="{Supplier}"/> 
				<Text text="{SupplierName}"/> 
			</cells>
		</ColumnListItem>
		<columns>
			<Column >
				<header>
					<Text text="{i18n>supplier}"/>
				</header>
			</Column>
			<Column >
				<header>
					<Text text="{i18n>supplierName}"/>
				</header>
			</Column>
		</columns>
		
	</TableSelectDialog>
</core:FragmentDefinition>